{
  "name": "rpc-explorer",
  "version": "1.2.2",
  "description": "Explorer for Bitcoin and RPC-compatible blockchains",
  "private": false,
  "bin": "bin/cli.js",
  "scripts": {
    "start": "node --max_old_space_size=8096 ./bin/www",
    "refresh-mining-pool-configs": "node ./bin/refresh-mining-pool-configs.js"
  },
  "keywords": [
    "bitcoin",
    "litecoin",
    "pigeoncoin",
    "raptoreum",
    "mynt",
    "blockchain"
  ],
  "author": "Tri Nguyen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joenilan/rpc-explorer.git"
  },
  "dependencies": {
    "async": "2.6.2",
    "basic-auth": "^2.0.1",
    "bitcoin-core": "2.0.0",
    "bitcoinjs-lib": "5.1.6",
    "body-parser": "~1.19.0",
    "cookie-parser": "^1.4.4",
    "crypto-js": "3.1.9-1",
    "csurf": "^1.10.0",
    "debug": "~4.1.1",
    "decimal.js": "10.1.1",
    "dotenv": "^8.1.0",
    "electrum-client": "github:chaintools/node-electrum-client#43a999036f9c5",
    "express": "^4.17.1",
    "express-rate-limit": "^5.0.0",
    "express-session": "1.16.1",
    "helmet": "^3.21.2",
    "i18n": "^0.8.4",
    "jstransformer-markdown-it": "^2.1.0",
    "lru-cache": "5.1.1",
    "marked": "0.7.0",
    "meow": "^5.0.0",
    "moment": "^2.24.0",
    "moment-duration-format": "2.2.2",
    "morgan": "^1.9.1",
    "pug": "2.0.3",
    "qrcode": "1.3.3",
    "redis": "2.8.0",
    "request": "2.88.0",
    "serve-favicon": "^2.5.0",
    "simple-git": "1.112.0"
